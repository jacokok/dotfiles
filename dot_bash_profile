# .bash_profile

# Get the aliases and functions
if [ -f ~/.bashrc ]; then
    . ~/.bashrc
fi

# User specific environment and startup programs
export EDITOR=hx

alias pm="pnpm"

if [ -e /home/doink/.nix-profile/etc/profile.d/nix.sh ]; then . /home/doink/.nix-profile/etc/profile.d/nix.sh; fi # added by Nix installer
